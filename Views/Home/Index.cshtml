@{
    ViewBag.Title = "Home Page";
    var articles = ViewBag.Articles;
}

<div class="jumbotron">
    <h1>Fil D'Actualité</h1>
    <p class="lead">Choisi ton actu frère</p>
</div>

<div class="row">
    @foreach (var article in articles)
    {
        <div>
            <h4>@article.Title</h4>
        </div>
        
    }
</div>